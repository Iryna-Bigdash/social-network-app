{
  "name": "social-network-app",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web"
  },
  "expo": {
    "android": {
      "permissions": [
        "CAMERA"
      ]
    },
    "ios": {
      "infoPlist": {
        "NSCameraUsageDescription": "We need your permission to use the camera"
      }
    }
  },
  "dependencies": {
    "@expo/vector-icons": "^13.0.0",
    "@firebase/auth": "^1.2.0",
    "@firebase/firestore": "^4.1.1",
